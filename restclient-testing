:host = http://127.0.0.1:3000

# Gets expenditures

GET :host/expenditure

# Gets expenditures

GET :host/expenditure/1

# Gets expenditures within time range

GET :host/expenditure/2000-01-01/2025-01-02

# Creates a new expenditure

POST :host/expenditure

{ "name": "chello world!", "amount": 10.0 }

# Deletes expenditure
DELETE :host/expenditure/1

# Retrieve JSON web token
POST :host/grant
Content-Type: application/json
{"email":"john@doe.com","pw":"thisistrulysafe"}

# Register new user
POST :host/register
Content-Type: application/json
{"email":"john@doe.com","pw":"thisistrulysafe"}

